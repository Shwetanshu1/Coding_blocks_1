import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
		int num;
		int num1=0;
		int num2=1;
		int num3; 
		Scanner s = new Scanner(System.in);
		num = s.nextInt();
		for(int i=2 ; i<=num; i++){
			num3 = num1+num2;
			num1 = num2;
			num2 = num3;
			if(i==num){
				System.out.println(num3);
			}
		}
    }
}
