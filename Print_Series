import java.util.*;
public class Main {
	public static void series(int num1, int num2){
		int count = 0;
		int n =1;
		while (count<num1){
			int sum = (3*n)+2;
			if (sum%num2!=0){
				System.out.println(sum);
				count++;
			}
			n++;
		}
	}
    public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n1= s.nextInt();
		int n2= s.nextInt();
		series(n1,n2);
    }
}
