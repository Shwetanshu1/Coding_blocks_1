import java.util.*;
public class Main {
    public static void main(String args[]) {
		int num1;
		int count=1;
		Scanner sc = new Scanner(System.in);
		num1 = sc.nextInt();
    	for (int i=2 ; i<=Math.sqrt(num1); i++){
			if(num1%i==0){
				count++;
			}
		}
		if(count==1){
			System.out.println("Prime");
		}else{
			System.out.println("Not Prime");
		}
		sc.close();
    }
}
